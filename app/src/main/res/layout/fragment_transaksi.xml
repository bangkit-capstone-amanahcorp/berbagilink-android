                        android:layout_height="wrap_content"
                        android:padding="12dp"
                        android:scrollbars="horizontal">

                        <TableLayout
                            android:id="@+id/tableLayout"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content">

                            <!-- Table Header Row -->
                            <TableRow
                                android:id="@+id/tableRowHeader"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
